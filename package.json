{
	"name": "hashed-out",
	"version": "1.0.0",
	"main": "index.js",
	"type": "module",
	"license": "MIT",
	"prettier": "@leonzalion/configs/prettier.cjs",
	"commitlint": {
		"extends": [
			"@leonzalion/configs/commitlint.cjs"
		]
	},
	"scripts": {
